.endTurnPane {
    width: 200px;
    height: 100px;
    right: 0;
    bottom: 0;
    position: absolute;
    background-color: rgb(0, 0, 64);
    justify-content: center;
    align-items: center;
}

#endTurnButton {
    font-size: 20px;
    -unity-font-style: bold;
    color: rgb(204, 204, 122);
}

#endTurnButton:hover {
    color: rgb(229, 229, 137);
}
